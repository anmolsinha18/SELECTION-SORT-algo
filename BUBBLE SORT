#include <stdio.h>
void main () {
    int ar[5]={5,4,3,2,1};
    int i;
    int j;
    int temp;
    printf("bubble sort:-\n");
    for(i=0;i<=4;i++)
    printf("%d\t",ar[i]);
    for (i=0;i<=3;i++){
        for(j=0;j<=3-i;j++){
            if((ar[j]>ar[j+1])){
            temp=ar[j];
            ar[j]=ar[j+1];
            ar[j+1]=temp;
        }
    }
}
    printf("\nafter sorting:-\n");
    for(i=0;i<=4;i++)
    printf("%d\t",ar[i]);
}
